{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell Launch AutoGenerateClientLibrariesLocal",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}/AutoGenerateClientLibrariesLocal.ps1",
      "cwd": "${cwd}",
      "args": [
        "-ApiVersionTag", "package-preview-2025-01", "-Branch", "release-securityinsights-Microsoft.SecurityInsights-2025-01-01-preview-Collaboration"
      ],
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--timeout",
        "999999",
        "--colors",
        "${workspaceFolder}/test/syntax.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${file}"
    }
  ]
}
